https://leetcode.com/problems/single-number/

class Solution {
    public int singleNumber(int[] nums) {
        Arrays.sort(nums);
        int i;
        for(i=0;i<nums.length-1;i+=2){
            if(nums[i]==nums[i+1]){
                continue;
            }
            else{
                break;
            }
        }
        return nums[i] ;
    }
}
